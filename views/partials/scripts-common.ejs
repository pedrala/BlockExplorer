	/**
	 * search token symbol/token Name/account id
	 * @event searchingEngine
	 * @param search
	 */
	async function searchingEngine(search) {

		Obj.tokenSymbol = search;
		Obj.tokenName = search;
		Obj.accountId = search;

		try {
			//Calling APIs in in parallel takes less time.
			//Calling APIs sequentially takes time * 3
			let tknSymRst = searchTokenDetailInfoByTknSym(search)
			let tknNameRst = searchTokenDetailInfoByTknName(search)
			let accIdRst = searchAccountDetailInfoByAccId(search)

			let a = await tknSymRst
			let b = await tknNameRst
			let c = await accIdRst

			// console.log("tknSymRst:" + a)	
			// console.log("tknNameRst:" + b)	
			// console.log("accIdRst:" + c)	

			if (a == true) {
				location.href = WEB_API + '/token-details/' + search;

			} else if (b == true) {
				location.href = WEB_API + '/token-details/' + search + "name";

			} else if (c == true) {
				location.href = WEB_API + '/account-details/' + search;

			} else {
				location.href = WEB_API + '/error-page/' + 'default';
			}

		} catch (err) {
			//	console.log("Fail to get result:", err);
			location.href = WEB_API + '/error-page/' + 'default';
		}
	}

